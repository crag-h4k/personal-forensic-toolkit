URxvt*termName: rxvt

URxvt.secondaryWheel: 1
Xft.antialias: 1
Xft.hinting: 1
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.dpi: 100
! URxvt
! General settings
URxvt.saveLines: 2000
URxvt.scrollBar: False
URxvt.scrollstyle: rxvt
URxvt.termName: rxvt-unicode-256color
URxvt.urgentOnBell: True
URxvt.cursorBlink: False

URxvt.dpi: 100
!annoying yellow box

URxvt.iso14755 : false

! Extensions
URxvt.perl-ext-common: default,clipboard,keyboard-select,url-select
URxvt.urlLauncher: xdg-open
URxvt.urlButton: 1
URxvt.underlineURLs: True


! Keybindings
!URxvt.keysym.Shift-Control-V: perl:clipboard:copy
!URxvt.keysym.Shift-Control-C: perl:clipboard:paste

URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

URxvt*buffered: false
URxvt.font:xft:hack-18
URxvt.boldFont:xft:hack-18
URxvt.letterSpace: 0

urxvt*depth: 32
! special

! special
*.foreground:   #58d982
*.background:   [80]#000000
*.cursorColor:  #58d982

! black
*.color0:       #000000
*.color8:       #686868

! red
*.color1:       #c91b00
*.color9:       #ff6e67

! green
*.color2:       #00c200
*.color10:      #5ffa68

! yellow
*.color3:       #c7c400
*.color11:      #fffc67

! blue
*.color4:       #027fc7
*.color12:      #6871ff

! magenta
*.color5:       #ca30c7
*.color13:      #ff77ff

! cyan
*.color6:       #00c5c7
*.color14:      #60fdff

! white
*.color7:       #c7c7c7
*.color15:      #ffffff

URxvt.transparent: true
URxvt.shading: 20
